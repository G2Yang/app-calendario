<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Borde circular exterior -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#f5b300"/> <!-- Color del borde exterior -->
        </shape>
    </item>

    <!-- Círculo interior más pequeño -->
    <item android:right="10dp" android:left="10dp" android:top="10dp" android:bottom="10dp">
        <shape android:shape="oval">
            <solid android:color="#e3d5af" /> <!-- Color del círculo interior -->
        </shape>
    </item>

</layer-list>
